interface Family {
    surname: string;
    location: string | null;
    members: Person[];
}

interface Person {
    name: string;
    age: number;
    employed: boolean;
    profession?: string;
}

interface Player {
    id: number;
    name: string;
    coords: {
        cartesian: {
            x: number;
            y: number;
        };
        polar: {
            r: number;
            theta: number;
        };
    };
}
